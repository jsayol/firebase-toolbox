{
  "compilerOptions": {
    "module": "commonjs",
    "sourceMap": true,
    "declaration": false,
    "outDir": "./dist",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "types": ["node", "../app-angular/src/@types/firebase-tools"],
    "lib": ["es2017", "es2016", "es2015", "dom"]
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
